{
  "extends": [
    "tslint:latest",
    "tslint-config-prettier"
  ],
  "rules": {
    "interface-name": [
      true,
      "never-prefix"
    ],
    "no-implicit-dependencies": [
      true,
      "dev"
    ],
    "no-var-keyword": true,
    "typedef": [
      true,
      "call-signature"
    ],
    "no-empty": false,
    "ordered-imports": false,
    "object-literal-sort-keys": false,
    "no-any": false,
    "no-inferrable-types": true
  }
}
